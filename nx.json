{
  "npmScope": "nx-dotnet-workspace",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nx-dotnet/core"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nx-dotnet/core:application": {
      "language": "C#",
      "tags": "type:api",
      "template": "webapi",
      "testTemplate": "xunit"
    },
    "@nx-dotnet/core:library": {
      "language": "C#",
      "template": "classlib",
      "testTemplate": "xunit"
    }
  },
  "plugins": [
    "@nx-dotnet/core"
  ]
}
